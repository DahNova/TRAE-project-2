  "version": "1.0.0",
  "description": "Backend service for URL crawler with SEO analysis",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
